{
  "name": "wicm",
  "version": "1.0.0",
  "description": "Web Interface Container Management",
  "scripts": {
    "test": "eslint .",
    "build": "webpack --mode=production --node-env=production",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "watch": "webpack --watch",
    "watch:dev": "webpack serve",
    "serve": "webpack serve --hot",
    "dev": "npm run build:dev && node build/bundle.js",
    "dev-reload": "concurrently \"webpack --watch\" \"nodemon -q --inspect --watch src -e ts --exec npm run dev\"",
    "start-reload": "node dev-server.js --open",
    "start-server": "nodemon ./build/app.bundle.js",
    "start-client": "set NODE_ENV=development && webpack --mode development --watch",
    "start-all": "set NODE_ENV=development && concurrently \"webpack --mode development --watch\" \"nodemon ./build/app.bundle.js\"",
    "build-css": "npx tailwindcss -i ./public/dist/css/global.css -o ./public/dist/css/global-compiled.css",
    "schema": "npx prisma generate --schema src/prisma/db.schema",
    "migrate": "npx prisma migrate dev --schema src/prisma/db.schema --skip-seed",
    "push": "npx prisma db push --schema src/prisma/db.schema"
  },
  "prisma": {
    "seed": "npx ts-node -r tsconfig-paths/register --compiler-options {\"module\":\"CommonJS\"} src/prisma/seed.ts"
  },
  "author": "elanor.melo@gmail.com",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^4.15.0",
    "@pulumi/docker": "^4.2.0",
    "@tailwindcss/forms": "^0.5.3",
    "cookie-parser": "^1.4.6",
    "crypto": "^1.0.1",
    "debug": "^4.3.4",
    "ejs": "~2.6.1",
    "express": "~4.18.2",
    "express-jwt": "^8.4.1",
    "express-session": "^1.17.3",
    "http-errors": "~2.0.0",
    "jquery": "^3.7.0",
    "jwt-decode": "^3.1.2",
    "morgan": "~1.10.0",
    "multer": "^1.4.5-lts.1",
    "multiparty": "^4.2.3",
    "tailwindcss": "^3.3.2",
    "twig": "^1.16.0"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/debug": "^4.1.7",
    "@types/dotenv-webpack": "^7.0.4",
    "@types/ejs": "^3.1.2",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/http-errors": "^2.0.1",
    "@types/jquery": "^3.5.16",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
